<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/eka"
        android:typeface="serif"
        android:textStyle="bold"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textAlignment="center"

    />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama     : Eka Febriyanti"
        android:textStyle="bold"
        android:typeface="serif"
        android:textSize="14dp"
        android:layout_marginLeft="8dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
     />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NIM        : 3.34.18.0.09"
        android:textStyle="bold"
        android:typeface="serif"
        android:layout_marginLeft="8dp"
        android:textSize="14dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TTL         : Batang, 01 Februari 2001"
        android:textStyle="bold"
        android:typeface="serif"
        android:layout_marginLeft="8dp"
        android:textSize="14dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Prodi      : D3 Teknik Informatika"
        android:textStyle="bold"
        android:layout_marginLeft="8dp"
        android:typeface="serif"
        android:textSize="14dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cita-cita : Membahagiakan orang tua"
        android:textStyle="bold"
        android:typeface="serif"
        android:textSize="14dp"
        android:layout_marginLeft="8dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        />
    
</LinearLayout>